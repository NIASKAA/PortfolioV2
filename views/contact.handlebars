 <header class="projectImage" style="background-image: url('/public/images/background4.jpg')">
      <div class="container position-relative px-4 px-lg-5">
          <div class="row gx-4 gx-lg-5 justify-content-center">
              <div class="col-md-10 col-lg-8 col-xl-7">
                  <div class="mainText">
                      <h1>Contact</h1>
                  </div>
              </div>
          </div>
      </div>
  </header>

  <section class="contactForms">
   <div class="btnGroup">
      <ul>
        <li>
          <a href="btn" class="fa fa-linkedin">LinkedIn</a>
        </li>
        <li>
          <a href="btn" class="fa fa-github">GitHub</a>
        </li>
        <li>
          <a href="btn">Resume</a>
        </li>
      </ul>
    </div>

    <div class="emailForm">
      <h4 class="emailTitle text-center">If you prefer to email me instead..</h4>
      <form>
        <div class="mb-3">
          <label for="InputEmail" class="form-label">Email address</label>
          <input type="form-control email" class="form-control">
        </div>
        <div class="mb-3">
          <label for="InputPassword" class="form-label">Name</label>
          <input type="form-control name" class="form-control">
        </div>
        <div class="mb-3">
          <label for="FormControlTextarea" class="form-label">Message</label>
          <textarea class="form-control message" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Send It</button>
      </form>
    </div>
  </section>